{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":preserveSemverRanges",
    ":rebaseStalePrs",
    ":disableDependencyDashboard",
    ":automergeBranch",
    ":automergePatch",
    ":automergeMinor",
    "github>cucumber/renovate-config:gemspec",
    "github>cucumber/renovate-config:disable-perl"
  ],
  "prHourlyLimit": 0
}